#include<stdio.h>
void main(){
    int n,i,j;
    printf("Enter the size:");
    scanf("%d",&n);
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            if (j == 0 || j == n-1 || i == j) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}
