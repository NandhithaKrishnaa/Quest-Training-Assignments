#include<stdio.h>
#include<math.h>
void main(){
    int a,b,c,res,ans1,ans2;
    printf("Enter a:");
    scanf("%d",&a);
    printf("Enter b:");
    scanf("%d",&b);
    printf("Enter c:");
    scanf("%d",&c);
    res=((b*b)-(4*a*c));
    ans1=(((-b)+sqrt(res))/(2*a));
    ans2=(((-b)-sqrt(res))/(2*a));
    printf("solution is:%d & %d",ans1,ans2);
}
